<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.UploadScoring_Controller">
   <top>
      <AnchorPane prefHeight="700.0" prefWidth="1050.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="modelanswer" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#init2" prefHeight="32.0" prefWidth="86.0" text="模範解答">
               <font>
                  <Font size="14.0" />
               </font></Button>
            <Button fx:id="studentanswer" layoutX="14.0" layoutY="334.0" mnemonicParsing="false" onAction="#init1" prefHeight="32.0" prefWidth="86.0" text="学生解答">
               <font>
                  <Font size="14.0" />
               </font></Button>
            <Button fx:id="scoring" layoutX="894.0" layoutY="622.0" mnemonicParsing="false" onAction="#goscoring" prefHeight="60.0" prefWidth="120.0" text="採点">
               <font>
                  <Font size="24.0" />
               </font></Button>
            <Button fx:id="trunback" layoutX="954.0" layoutY="25.0" mnemonicParsing="false" onAction="#ontrunbackbutton" prefHeight="32.0" prefWidth="65.0" text="戻る" />
            <TextField fx:id="modelpath" layoutX="112.0" layoutY="16.0" prefHeight="26.0" prefWidth="566.0" />
            <TextField fx:id="studentpath" layoutX="112.0" layoutY="337.0" prefHeight="26.0" prefWidth="566.0" />
            <TableView fx:id="modeltable" layoutX="14.0" layoutY="57.0" prefHeight="223.0" prefWidth="286.0">
              <columns>
                <TableColumn fx:id="numbercolumn" prefWidth="95.0" text="問題番号" />
                <TableColumn fx:id="answercolumn" prefWidth="95.0" text="解答" />
                  <TableColumn fx:id="pointcolumn" prefWidth="95.0" text="配点" />
              </columns>
            </TableView>
            <TableView fx:id="studenttable" layoutX="16.0" layoutY="382.0" prefHeight="304.0" prefWidth="758.0">
              <columns>
                <TableColumn prefWidth="75.0" text="C1" />
                <TableColumn prefWidth="75.0" text="C2" />
              </columns>
            </TableView>
            <Button fx:id="modeldelete" layoutX="687.0" layoutY="13.0" mnemonicParsing="false" onAction="#Model_onClickDelete" prefHeight="32.0" prefWidth="60.0" text="消去" />
            <Button fx:id="studentdelete" layoutX="687.0" layoutY="334.0" mnemonicParsing="false" onAction="#Student_onClickDelete" prefHeight="32.0" prefWidth="60.0" text="消去" />
            <ComboBox fx:id="year" layoutX="100.0" layoutY="298.0" prefWidth="150.0" promptText="開講年度" />
            <ComboBox fx:id="subject" layoutX="265.0" layoutY="297.0" prefHeight="26.0" prefWidth="191.0" promptText="コース名" />
            <Label layoutX="16.0" layoutY="302.0" text="保存先パス">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <ComboBox fx:id="examination" layoutX="471.0" layoutY="298.0" prefWidth="150.0" promptText="試験名" />
         </children>
      </AnchorPane>
   </top>
</BorderPane>
